				<RegExpr attribute="Integer" context="Argument End" String="0*(?:2(?:5[0-5]|[0-4]\d)|1?\d{1,2})"/>
				<DetectChar attribute="Normal" context="#pop!Boolean at End" char=" "/>
			</context>

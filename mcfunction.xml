				<WordDetect attribute="Normal" context="End" String="destroy"/>
				<WordDetect attribute="Normal" context="End" String="hollow"/>
				<WordDetect attribute="Normal" context="End" String="outline"/>
				<WordDetect attribute="Normal" context="End" String="keep"/>
				<WordDetect attribute="Normal" context="Argument End" String="replace"/>
				<DetectChar attribute="Normal" context="#pop!Fill Command 5" char=" "/>

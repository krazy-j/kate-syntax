				<Int attribute="Integer" context="End"/>
			</context>
			
			<context name="Effect Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<WordDetect attribute="Normal" context="#pop!Effect Give Command Selector" String="give "/>
				<WordDetect attribute="Normal" context="#pop!Effect Clear Command Selector" String="clear "/>
				<WordDetect attribute="Normal" context="End" String="clear"/>
			</context>
			<context name="Effect Give Command Selector" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Entity Selector"/>
				<DetectChar attribute="Normal" context="#pop!Effect Give Command Effect" char=" "/>
			</context>
			<context name="Effect Give Command Effect" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Status Effect"/>
				<DetectChar attribute="Normal" context="#pop!Effect Give Command Duration" char=" "/>

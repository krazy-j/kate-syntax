			<context name="Item">
				<RegExpr context="Item Namespace Highlight" lookAhead="true" String="(\w+):\w+"/>
				<RegExpr attribute="Item" context="Item Argument End" String="\w+"/>
			</context>
			<context name="Item Namespace Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<StringDetect attribute="Namespace" String="%1" dynamic="true"/>
				<DetectChar context="#pop!Item Highlight" char=":"/>
			</context>
			<context name="Item Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<RegExpr attribute="Item" context="#pop!Item Argument End" String="\w+"/>
			</context>
			<context name="Item Tag">
				<RegExpr context="Item Namespace Highlight" lookAhead="true" String="#(\w+):\w+"/>
				<RegExpr context="Item Highlight" lookAhead="true" String="#\w+"/>
			</context>
			<context name="Item Argument End" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Brace" context="Dictionary" char="{"/>
				<IncludeRules context="Argument End"/>
			</context>
			
			<context name="Block">
				<RegExpr context="Block Namespace Highlight" lookAhead="true" String="(\w+):\w+"/>
				<RegExpr attribute="Block" context="Block Argument End" String="\w+"/>
			</context>
			<context name="Block Namespace Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<StringDetect attribute="Namespace" String="%1" dynamic="true"/>
				<DetectChar context="#pop!Block Highlight" char=":"/>
			</context>
			<context name="Block Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<RegExpr attribute="Block" context="#pop!Block Argument End" String="\w+"/>
			</context>
			<context name="Block Tag">
				<RegExpr context="Block Namespace Highlight" lookAhead="true" String="#(\w+):\w+"/>
				<RegExpr context="Block Highlight" lookAhead="true" String="#\w+"/>
			</context>
			<context name="Block Argument End" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Block" context="Block State List" char="["/>
				<DetectChar attribute="Brace" context="Dictionary" char="{"/>
				<IncludeRules context="Argument End"/>
			</context>
			<context name="Block State List" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Block" context="#pop" char="]"/>
				<RegExpr context="Block State" lookAhead="true" String="([a-z_]+)=(?:[^,=\]]+)"/>
			</context>
			<context name="Block State" lineEndContext="#pop">
				<StringDetect attribute="Block State" String="%1" dynamic="true"/>
				<DetectChar attribute="Operator" char="="/>
				<DetectChar attribute="List Separator" context="#pop" char=","/>
				<DetectChar attribute="Block" context="#pop#pop" char="]"/>
				<WordDetect attribute="True" String="true"/>
				<WordDetect attribute="False" String="false"/>
				<keyword attribute="Direction" String="direction"/>
				<DetectChar attribute="X" char="x"/>
				<DetectChar attribute="Y" char="y"/>
				<DetectChar attribute="Z" char="z"/>
			</context>
			
			<context name="Entity">
				<RegExpr context="Entity Highlight" lookAhead="true" String="(\w+):(\w+)"/>
				<keyword attribute="Entity" context="Argument End" String="\w+"/>
			</context>
			<context name="Entity Highlight" lineEndContext="#pop">
				<StringDetect attribute="Namespace" String="%1" dynamic="true"/>
				<StringDetect attribute="Entity" context="#pop!Argument End" String="%2" dynamic="true"/>
			</context>
			
			<context name="Status Effect">
				<RegExpr context="Status Effect Highlight" lookAhead="true" String="(\w+):(\w+)"/>
				<keyword attribute="Status Effect" context="Argument End" String="status effect"/>
			</context>
			<context name="Status Effect Highlight" lineEndContext="#pop">
				<StringDetect attribute="Namespace" String="%1" dynamic="true"/>
				<StringDetect attribute="Status Effect" context="#pop!Argument End" String="%2" dynamic="true"/>
			</context>
			
			<context name="Resource Location">
				<RegExpr context="Resource Location Namespace Highlight" lookAhead="true" String="(\w+):\w+(?:/\w+)*"/>
				<RegExpr context="Resource Location Path Highlight" lookAhead="true" String="\w+(?:/\w+)*"/>
			</context>
			<context name="Resource Location Tag">
				<RegExpr context="Resource Location Namespace Highlight" lookAhead="true" String="#(\w+):\w+(?:/\w+)*"/>
				<RegExpr context="Resource Location Path Highlight" lookAhead="true" String="#\w+(?:/\w+)*"/>
			</context>
			<context name="Resource Location Namespace Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<StringDetect attribute="Namespace" String="%1" dynamic="true"/>
				<DetectChar attribute="Normal" context="#pop!Resource Location Path Highlight" char=":"/>
			</context>
			<context name="Resource Location Path Highlight" lineEndContext="#pop">
				<DetectChar attribute="Tag" char="#"/>
				<RegExpr attribute="Path" String="[0-9a-z_.-]+"/>
				<DetectChar attribute="Normal" char="/"/>
				<DetectChar context="#pop" lookAhead="true" char=" "/>
			</context>
			<context name="Function" lineEndContext="#pop">
				<RegExpr context="Resource Location Namespace Highlight" lookAhead="true" String="#?(\w+):\w+(?:/\w+)*"/>
			</context>
			
			<!-- commands -->
			<context name="Execute Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<WordDetect attribute="Execute Subcommand" context="Execute Anchored" String="anchored "/>
				<WordDetect attribute="Execute Subcommand" context="Execute Align" String="align "/>
				<WordDetect attribute="Execute Subcommand" context="Entity Selector Popback" String="as "/>
				<WordDetect attribute="Execute Subcommand" context="Entity Selector Popback" String="at "/>
				<WordDetect attribute="Execute Subcommand" context="Entity Selector Popback" String="positioned as "/>
				<WordDetect attribute="Execute Subcommand" context="Position Popback" String="positioned "/>
				<WordDetect attribute="Execute Subcommand" context="Entity Selector Popback" String="rotated as "/>
				<WordDetect attribute="Execute Subcommand" context="#pop!Begin" String="run "/>
				<keyword attribute="Execute Subcommand" context="Execute Generic" String="execute subcommand"/>
			</context>
			<context name="Execute Generic" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<keyword context="#pop" lookAhead="true" String="execute subcommand"/>
				<IncludeRules context="Generic Command"/>
			</context>
			<context name="Execute Align" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<DetectChar attribute="X" char="x"/>
				<DetectChar attribute="Y" char="y"/>
				<DetectChar attribute="Z" char="z"/>
				<DetectChar attribute="Normal" context="#pop" char=" "/>
			</context>
			<context name="Execute Anchored" attribute="Error" lineEndContext="#pop">
				<WordDetect attribute="Normal" context="#pop" String="eyes "/>
				<WordDetect attribute="Normal" context="#pop" String="feet "/>
			</context>
			
			<context name="Function Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Function"/>
				<DetectChar context="End" char=" "/>
			</context>
			
			<context name="Schedule Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<WordDetect attribute="Normal" context="#pop!Schedule Command 2" String="function "/>
				<WordDetect attribute="Normal" context="#pop!Function Command" String="clear "/>
			</context>
			<context name="Schedule Command 2" attribute="Error" lineEndContext="#pop">

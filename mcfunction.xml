			</context>
			
			<context name="Clear Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Entity Selector"/>
				<DetectChar attribute="Normal" context="#pop!Clear Command Item" char=" "/>

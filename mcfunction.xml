				<DetectChar attribute="Normal" context="#pop!Schedule Command 4" char=" "/>
			</context>
			<context name="Schedule Command 4" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<WordDetect attribute="Normal" context="End" String="append"/>
				<WordDetect attribute="Normal" context="End" String="replace"/>
			</context>
			
			<context name="Difficulty Command" attribute="Error" lineEndContext="#pop">
				<keyword attribute="Normal" context="End" String="difficulty"/>
			</context>
			
			<context name="Default Gamemode Command" attribute="Error" lineEndContext="#pop">
				<keyword attribute="Game Mode" context="End" String="gamemode"/>
			</context>
			
			<context name="Gamemode Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<keyword attribute="Game Mode" context="Argument End" String="gamemode"/>
				<DetectChar attribute="Normal" context="Entity Selector at End" char=" "/>
			</context>
			
			<context name="Gamerule Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<keyword attribute="Game Rule" context="Gamerule Command Boolean" String="boolean gamerule"/>
				<keyword attribute="Game Rule" context="Gamerule Command Integer" String="integer gamerule"/>
			</context>
			<context name="Gamerule Command Boolean" attribute="Error" lineEndContext="#pop">

			<context name="Fill Command 5" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Block"/>
				<IncludeRules context="Block Tag"/>
				<DetectChar context="End" char=" "/>
			</context>
			
			<context name="Clone Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Block Position"/>
				<DetectChar attribute="Normal" context="#pop!Clone Command 2" char=" "/>

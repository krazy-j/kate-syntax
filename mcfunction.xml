		<contexts>
			<context name="Begin" attribute="Error" lineEndContext="#pop">
				<DetectSpaces attribute="Normal"/>
				<IncludeRules context="Global"/>
				<DetectChar attribute="Comment" context="Comment" char="#"/>
				<WordDetect attribute="Command" context="#pop!Execute Command" String="execute "/>
				<!--zero-argument commands-->
				<WordDetect attribute="Command" context="#pop!End" String="seed"/>
				<WordDetect attribute="Command" context="#pop!End" String="reload"/>
				<!--misc-->
				<WordDetect attribute="Command" context="#pop!Function Command" String="function "/>
				<WordDetect attribute="Command" context="#pop!Schedule Command" String="schedule "/>
				<WordDetect attribute="Command" context="#pop!Gamerule Command" String="gamerule "/>
				<WordDetect attribute="Command" context="#pop!Difficulty Command" String="difficulty "/>
				<WordDetect attribute="Command" context="#pop!Default Gamemode Command" String="defaultgamemode "/>
				<WordDetect attribute="Command" context="#pop!Entity Selector at End" String="kill "/>
				<!--player-specific commands-->
				<WordDetect attribute="Command" context="#pop!Gamemode Command" String="gamemode "/>
				<WordDetect attribute="Command" context="#pop!Give Command" String="give "/>
				<WordDetect attribute="Command" context="#pop!Clear Command" String="clear "/>
				<WordDetect attribute="Command" context="#pop!Effect Command" String="effect "/>
				<WordDetect attribute="Command" context="#pop!Advancement Command" String="advancement "/>
				<!--block commands-->
				<WordDetect attribute="Command" context="#pop!Setblock Command" String="setblock "/>
				<WordDetect attribute="Command" context="#pop!Fill Command" String="fill "/>
				<WordDetect attribute="Command" context="#pop!Clone Command" String="clone "/>
				<!--message related commands-->
				<WordDetect attribute="Command" context="#pop!Text" String="say"/>
				<WordDetect attribute="Command" context="#pop!Text" String="me"/>
				<WordDetect attribute="Command" context="#pop!Text" String="tm"/>
				<WordDetect attribute="Command" context="#pop!Text" String="teammsg"/>
				<WordDetect attribute="Command" context="#pop!Whisper Command" String="tell"/>
				<WordDetect attribute="Command" context="#pop!Whisper Command" String="msg"/>
				<WordDetect attribute="Command" context="#pop!Whisper Command" String="w"/>
				<WordDetect attribute="Command" context="#pop!Tellraw Command" String="tellraw "/>
				
				<keyword attribute="Command" context="#pop!Generic Command" String="command"/>
			</context>
			<context name="Comment" attribute="Comment" lineEndContext="#pop"/>
			<context name="Text" lineEndContext="#pop"/>
			<context name="Error" attribute="Error" lineEndContext="#pop"/>
			<context name="End" attribute="Error" lineEndContext="#pop"/>
			
			<context name="Global" lineEndContext="#pop">
				<RegExpr attribute="Error" String="  +| $"/>
			</context>
			
			<context name="Common" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<WordDetect attribute="True" String="true"/>
				<WordDetect attribute="False" String="false"/>
				<DetectChar attribute="String" context="String Single" char="'"/>
				<DetectChar attribute="String" context="String Double" char="&quot;"/>
				<RegExpr context="Coordinate 3" lookAhead="true" String="(?:~?-?\d+\.?\d*|~?-?\.\d+|~) (?:~?-?\d+\.?\d*|~?-?\.\d+|~) (?:~?-?\d+\.?\d*|~?-?\.\d+|~)"/>
				<RegExpr context="Local Coordinate 3" lookAhead="true" String="\^(?:-?\d+\.?\d*|-?\.\d+)? \^(?:-?\d+\.?\d*|-?\.\d+)? \^(?:-?\d+\.?\d*|-?\.\d+)?"/>
				<RegExpr attribute="Range" String="(?:-?\d*\.?\d+)?\.\.-?(?:\d+\.|\.?\d)\d*|-?\d*\.?\d+\.\."/>
				<RegExpr attribute="Float" context="Data Type Suffix" String="-?\d+\.\d*|-?\.\d+"/>
				<RegExpr attribute="Integer" context="Data Type Suffix" String="-?\d+"/>
				<keyword attribute="Operator" String="operator"/>
				<StringDetect context="JSON Text" lookAhead="true" String="[&quot;"/>
				<DetectChar attribute="Bracket" context="Array" char="["/>
				<StringDetect context="JSON Text" lookAhead="true" String="{&quot;"/>
				<DetectChar attribute="Brace" context="Dictionary" char="{"/>
				<AnyChar attribute="Error" String="]}"/>
			</context>
			
			<context name="Generic Command" lineEndContext="#pop">
				<IncludeRules context="Common"/>
				<RegExpr attribute="Entity Selector" context="Entity Selector Argument List" String="@[aeprs]\["/>
				<RegExpr attribute="Entity Selector" context="Argument End" String="@[aeprs]"/>
				<DetectChar attribute="Error" char="@"/>
				<keyword attribute="Data Type" String="data type"/>
			</context>
			
			
			<context name="String Single" attribute="String" lineEndContext="#pop">
				<HlCStringChar attribute="Special Character"/>
				<DetectChar attribute="String" context="#pop" char="'"/>
			</context>
			<context name="String Double" attribute="String" lineEndContext="#pop">
				<HlCStringChar attribute="Special Character"/>
				<DetectChar attribute="String" context="#pop" char="&quot;"/>
			</context>
			
			<context name="Array" lineEndContext="#pop">
				<DetectChar attribute="Error" char=" "/>
				<DetectChar attribute="List Separator" char=","/>
				<DetectChar attribute="Bracket" context="#pop" char="]"/>
				<IncludeRules context="Common"/>
			</context>
			<context name="Dictionary" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Brace" context="#pop" char="}"/>
				<RegExpr context="Dictionary Key" lookAhead="true" String="(?:\w+|&quot;.+&quot;)[:=]"/>
			</context>
			
			<context name="Dictionary Key" lineEndContext="#pop">
				<DetectChar attribute="Brace" context="#pop!Dictionary Value" char=":"/>
				<DetectChar attribute="Brace" context="#pop!Dictionary Value" char="="/>
				<IncludeRules context="Common"/>
			</context>
			
			<context name="Dictionary Value" lineEndContext="#pop">
				<DetectChar attribute="Brace" context="#pop#pop" char="}"/>
				<AnyChar attribute="Error" String=" :="/>
				<DetectChar attribute="List Separator" context="#pop" char=","/>
				<IncludeRules context="Common"/>
			</context>
			
			<context name="JSON Array" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Array##JSON"/>
			</context>
			<context name="JSON Dictionary" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Pair##JSON"/>
			</context>
			
			<!-- argument validation/highlighting rule sets -->
			<context name="Argument End" attribute="Error" lineEndContext="#pop">
				<StringDetect context="Error" String="  "/>
				<DetectChar context="#pop" lookAhead="true" char=" "/>
			</context>
			
			<context name="Boolean">
				<WordDetect attribute="True" context="Argument End" String="true"/>
				<WordDetect attribute="False" context="Argument End" String="false"/>
			</context>
			<context name="Integer">
				<RegExpr attribute="Integer" context="Argument End" String="-?\d+"/>
			</context>
			<context name="Float">
				<RegExpr attribute="Float" context="Argument End" String="-?\d+\.?\d*|-?\.\d+"/>
			</context>
			<context name="Integer Range">
				<RegExpr attribute="Range" context="Argument End" String="-?\d+\.\.(?:-?\d+)?|\.\.-?\d+"/>
			</context>
			<context name="Float Range">
				<RegExpr attribute="Range" context="Argument End" String="(?:-?\d*\.?\d+)?\.\.-?(?:\d+\.|\.?\d)\d*|-?\d*\.?\d+\.\."/>
			</context>
			
			<context name="Absolute Coordinate">
				<RegExpr attribute="Coordinate" context="Argument End" String="-?\d+\.?\d*|-?\.\d+"/>
			</context>
			<context name="Relative Coordinate">
				<RegExpr attribute="Relative Coordinate" context="Argument End" String="~-?(?:\d+\.|\.?\d)?\d*"/>
			</context>
			<context name="Local Coordinate">
				<RegExpr attribute="Local Coordinate" context="Argument End" String="\^-?(?:\d+\.?|\^\.\d)?\d*"/>
			</context>
			<context name="Block Coordinate">
				<RegExpr attribute="Coordinate" context="Argument End" String="-?\d+"/>
				<IncludeRules context="Relative Coordinate"/>
			</context>
			
			<context name="Data Type Suffix" fallthroughContext="#pop">
				<AnyChar attribute="Data Type" context="#pop" String="bsildf"/>
			</context>
			
			<context name="Boolean at End" attribute="Error" lineEndContext="#pop">
				<WordDetect attribute="True" context="End" String="true"/>
				<WordDetect attribute="False" context="End" String="false"/>
			</context>
			<context name="Integer at End" attribute="Error" lineEndContext="#pop">
				<RegExpr attribute="Integer" context="End" String="-?\d+"/>
			</context>
			
			<context name="JSON Text" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Bracket" context="#pop!JSON Array" char="["/>
				<DetectChar attribute="Brace" context="#pop!JSON Dictionary" char="{"/>
				<DetectChar attribute="String" context="#pop!String Double" char="&quot;"/>
			</context>
			
			<context name="Position">
				<IncludeRules context="Local Position"/>
				<AnyChar context="Coordinate 3" lookAhead="true" String="~-.0123456789"/>
			</context>
			<context name="Position Popback" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Position"/>
				<DetectChar attribute="Normal" context="#pop" char=" "/>
			</context>
			<context name="Coordinate 1" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Absolute Coordinate"/>
				<IncludeRules context="Relative Coordinate"/>
				<DetectChar context="#pop" lookAhead="true" char=" "/>
			</context>
			<context name="Coordinate 2" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Absolute Coordinate"/>
				<IncludeRules context="Relative Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Coordinate 1" char=" "/>
			</context>
			<context name="Coordinate 3" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Absolute Coordinate"/>
				<IncludeRules context="Relative Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Coordinate 2" char=" "/>
			</context>
			
			<context name="Local Position">
				<DetectChar context="Local Coordinate 3" lookAhead="true" char="^"/>
			</context>
			<context name="Local Coordinate 1" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Local Coordinate"/>
				<DetectChar context="#pop" lookAhead="true" char=" "/>
			</context>
			<context name="Local Coordinate 2" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Local Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Local Coordinate 1" char=" "/>
			</context>
			<context name="Local Coordinate 3" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Local Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Local Coordinate 2" char=" "/>
			</context>
			
			<context name="Block Position">
				<IncludeRules context="Local Position"/>
				<AnyChar context="Block Coordinate 3" lookAhead="true" String="~-0123456789"/>
			</context>
			<context name="Block Coordinate 1" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Block Coordinate"/>
				<DetectChar context="#pop" lookAhead="true" char=" "/>
			</context>
			<context name="Block Coordinate 2" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Block Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Block Coordinate 1" char=" "/>
			</context>
			<context name="Block Coordinate 3" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Block Coordinate"/>
				<DetectChar attribute="Normal" context="#pop!Block Coordinate 2" char=" "/>
			</context>
			
			<context name="Entity Selector">
				<RegExpr attribute="Entity Selector" context="Entity Selector Argument List" String="@[eaprs]\["/>
				<RegExpr attribute="Entity Selector" context="Argument End" String="@[eaprs]|[\w\.\+-]{1,16}"/>
			</context>
			<context name="Entity Selector Argument List" attribute="Error" lineEndContext="#pop">
				<DetectChar attribute="Entity Selector" context="#pop!Argument End" char="]"/>
				<RegExpr context="Entity Selector Argument" lookAhead="true" String="[a-z_]+=[^,=\]]*"/>
			</context>
			<context name="Entity Selector Argument" lineEndContext="#pop">
				<keyword attribute="Entity Selector Argument" String="selector argument"/>
				<DetectChar attribute="Operator" char="="/>
				<DetectChar attribute="List Separator" context="#pop" char=","/>
				<DetectChar attribute="Entity Selector" context="#pop#pop!Argument End" char="]"/>
				<IncludeRules context="Common"/>
			</context>
			
			<context name="Entity Selector Popback" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Entity Selector"/>
				<DetectChar attribute="Normal" context="#pop" char=" "/>
			</context>
			<context name="Entity Selector at End" attribute="Error" lineEndContext="#pop">

				<WordDetect attribute="Normal" context="End" String="destroy"/>
				<WordDetect attribute="Normal" context="End" String="keep"/>
				<WordDetect attribute="Normal" context="End" String="replace"/>
			</context>
			
			<context name="Fill Command" attribute="Error" lineEndContext="#pop">

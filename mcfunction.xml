				<DetectChar attribute="Normal" context="Integer at End" char=" "/>
			</context>
			
			<context name="Setblock Command" attribute="Error" lineEndContext="#pop">
				<IncludeRules context="Global"/>
				<IncludeRules context="Block Position"/>
				<DetectChar attribute="Normal" context="#pop!Setblock Command 2" char=" "/>
			</context>
			<context name="Setblock Command 2" attribute="Error" lineEndContext="#pop">
